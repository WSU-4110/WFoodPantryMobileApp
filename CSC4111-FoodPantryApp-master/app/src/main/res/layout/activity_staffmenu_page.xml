<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/staffmenu_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#93E9BE"
    tools:context=".StaffMenuPage">

    <Button
        android:id="@+id/pushUpdate"
        android:layout_width="127dp"
        android:layout_height="58dp"
        android:text="Push Update"
        android:textColor="#000000"
        app:backgroundTint="#FBC663"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkOrderButton"
        app:layout_constraintVertical_bias="0.138" />

    <Button
        android:id="@+id/checkOrderButton"
        android:layout_width="127dp"
        android:layout_height="57dp"
        android:layout_marginStart="4dp"
        android:layout_marginLeft="4dp"
        android:text="Check Orders"
        android:textColor="#000000"
        app:backgroundTint="#FBC663"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toStartOf="@+id/msgUserButton"
        app:layout_constraintHorizontal_bias="0.264"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.328" />

    <Button
        android:id="@+id/msgUserButton"
        android:layout_width="127dp"
        android:layout_height="58dp"
        android:layout_marginStart="116dp"
        android:layout_marginLeft="116dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="Message User"
        android:textColor="#000000"
        app:backgroundTint="#FBC663"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/checkOrderButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.325" />

    <Button
        android:id="@+id/LogOff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/log_off"
        app:backgroundTint="#FF0000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/aboutButton"
        app:layout_constraintVertical_bias="0.922" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="296dp"
        android:layout_height="285dp"
        android:src="@drawable/waynestate"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.735" />
</androidx.constraintlayout.widget.ConstraintLayout>